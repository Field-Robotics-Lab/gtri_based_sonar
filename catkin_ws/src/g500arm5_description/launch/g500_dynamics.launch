<launch>  
  <include file="$(find g500arm5_description)/launch/dynamics.launch">
    <arg name="namespace" value="g500" />
    <arg name="params" value="$(find g500arm5_description)/config/dynamics_g500.yaml" />
  </include>
  <rosparam param="/g500/dynamics/external_force_topic">g500/ForceSensor</rosparam>
  
  <arg name="thrusters_topic" default="/g500/thrusters_input" />
  <node name="keyboard_command" pkg="g500arm5_description" type="keyboard.py" args="$(arg thrusters_topic)" output="screen" respawn="false"/>                
</launch>

